{
  "Fields": [
    {
      "Name": "[ARTIST]",
      "RegEx": "(.+)"
    },
    {
      "Name": "[TITLE]",
      "RegEx": "(.+)"
    },
    {
      "Name": "[ALBUM]",
      "RegEx": "(.+)"
    }
  ],
  "Separators": [
    {
      "Name": "[-]",
      "RegEx": "\\s?\\-\\s?"
    },
    {
      "Name": "[S]",
      "RegEx": "\\s?"
    },
    {
      "Name": "[.]",
      "RegEx": "\\.?"
    }
  ],
  "Counters": [
    {
      "Name": "[0]",
      "RegEx": "\\d{1,3}"
    },
    {
      "Name": "[0].",
      "RegEx": "\\d{1,3}."
    }
  ],
  "CommonPatterns": [
      "[0][ARTIST][-][TITLE]",
      ""
      ]

}
